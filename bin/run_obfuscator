#!/usr/bin/env bash

set -o errexit
set -o pipefail
set -o nounset
# set -o xtrace

__root="$(git rev-parse --show-toplevel)"
__dir="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"

bazel build //src/main/java/com/example/obfuscator:ObfuscatorServer

pushd "${__root}"

${__root}/bazel-bin/src/main/java/com/example/obfuscator/ObfuscatorServer

popd
